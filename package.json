{
  "name": "fps-now",
  "version": "3.0.1",
  "description": "FPS meter for evergreen browsers",
  "main": "lib",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "speedrun ./example.js"
  },
  "keywords": [
    "fps",
    "performance",
    "stats",
    "frames",
    "measure",
    "tick",
    "meter"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/fps-now.git"
  },
  "license": "MIT",
  "devDependencies": {
    "2d-context": "^1.3.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "canvas-loop": "^1.0.7",
    "eases": "^1.0.8",
    "lerp": "^1.0.3",
    "speedrun": "0.2.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "canvas-fit": "^1.5.0",
    "eventemitter3": "^1.1.1",
    "fps": "0.0.3",
    "raf-loop": "^1.1.3"
  }
}
